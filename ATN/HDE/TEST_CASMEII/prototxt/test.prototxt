name: "MEGC"
layer {
  name: "data"
  type: "Python"
  top: "data_image"
  top: "label"
  top: "data_point"
  top: "index"
  python_param{
    module: "ReadImageAndPointData"
    layer: "ReadImageAndPointData_Layer"    
    param_str: "{\"root_folder\":\"Data/Apex_Cropped_images\",   \"source\":\"ATN/HDE/Test_CASMEII/Test_Apex.txt\",   \"state\":\"test\",   \"batch_size\":1,   \"color\":False,   \"shuffle\":False,   \"new_height\":224,   \"new_width\":224,   \"crop_height\":0,   \"crop_width\":0,    \"sourcepoint\":\"ATN/HDE/Test_CASMEII/Test_optical.txt\",   \"sequence_length\":64,   \"pointdim\": 128,          \"mean_value\":0.0,   \"scale\":1,          \"probability_value\":0,   \"mirror\":False,   \"rotation\":False, \"maxrotation\":5,   \"colorshift\":False, \"maxcolorshift\":10}"
  }
  include {
    phase: TEST
  }
}



