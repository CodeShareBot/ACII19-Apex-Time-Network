#################   Define net point  #############
###notice: point dim is T N D
###T is time
###N is batchsize
###D is feature dim

layer {
  name: "lstm1"
  type: "LSTM"
  bottom: "data_point"
  bottom: "index"
  top: "lstm1"
  recurrent_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "lstm2"
  type: "LSTM"
  bottom: "lstm1"
  bottom: "index"
  top: "lstm2"
  recurrent_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "Transpose_lstm2"
  type: "Permute"
  bottom: "lstm2"
  top: "Transpose_lstm2"
  permute_param {
    order: [1,2,0]
  }
}
layer {
  name: "Transpose_lstm2_reshape"
  type: "Reshape"
  bottom: "Transpose_lstm2"
  top: "Transpose_lstm2_reshape"
  reshape_param {
    shape {
      dim: 0
      dim: 0
      dim: 0
      dim: 1
    }
  }
}
layer {
  name: "optical_feature"
  type: "Pooling"
  bottom: "Transpose_lstm2_reshape"
  top: "optical_feature"
  pooling_param {
    pool: AVE
    global_pooling: true
  }
}

layer {
  name: "l2_optical"
  type: "L2Normalization"
  bottom: "optical_feature"
  top: "l2_optical"
}






#################concat classify#############
layer {
  name: "images_optical_Concat"
  type: "Concat"
  bottom: "l2_images"
  bottom: "l2_optical"
  top: "images_optical_Concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "images_optical_Concat_drop"
  type: "Dropout"
  bottom: "images_optical_Concat"
  top: "images_optical_Concat"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "classifier-Micro_expression"
  type: "InnerProduct"
  bottom: "images_optical_Concat"
  top: "classifier-Micro_expression"
  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0
  }
  inner_product_param {
    num_output: 3
    weight_filler {
      type: "xavier"
      std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0.2
    }
  }
}
layer {
  name: "accuracy-Micro_expression"
  type: "Accuracy"
  bottom: "classifier-Micro_expression"
  bottom: "label"
  top: "accuracy-Micro_expression"
  top: "accuracy-each"
  include {
    phase: TEST
  }
}

